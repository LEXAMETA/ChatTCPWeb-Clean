{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "installCommand": "echo 'Installing dependencies' && npm install --no-cache --verbose",
        "buildCommand": "echo 'Running build' && npm run web:build && npm run postbuild",
        "outputDirectory": "web-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "engines": {
    "node": "18.x"
  }
}
